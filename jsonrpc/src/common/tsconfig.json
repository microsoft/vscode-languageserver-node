{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "node",
        "rootDir": ".",
        "types": [],
        "strict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "declaration": true,
        "stripInternal": true,
        "sourceMap": true,
        "declarationMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "target": "es2022",
        "lib": [
            "es2022"
        ],
        "outDir": "../../lib/common",
        "tsBuildInfoFile": "../../lib/common/compile.tsbuildInfo",
        "incremental": true,
        "composite": true
    },
    "include": [
        "."
    ],
    "exclude": [
        "test"
    ]
}